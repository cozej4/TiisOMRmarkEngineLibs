<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="GIIS_URL" value="https://ivd.moh.go.tz/SVC"/>

  </appSettings>
  <system.diagnostics>
    <sharedListeners>
      <add name="file_log"
        type="System.Diagnostics.TextWriterTraceListener"
        initializeData="Scanner.log" />
    </sharedListeners>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <add name="file_log" />
      </listeners>
    </trace>
  </system.diagnostics>

  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
